<?xml version="1.0" encoding="UTF-8"?>
<annotatedContainer xmlns="https://annotated-container.cspray.io/schema/annotated-container.xsd">
  <scanDirectories>
    <source>
      <dir>src</dir>
    </source>
  </scanDirectories>
  <!-- Amp is a long-running process and does not really need caching, uncomment this to see what caching looks like -->
  <!-- <cacheDir>.annotated-container-cache</cacheDir> -->
  <containerDefinitionBuilderContextConsumer>Cspray\AnnotatedContainer\AmpHttpServer\ThirdPartyServicesProvider</containerDefinitionBuilderContextConsumer>
  <logging>
    <file>data/logs/annotated-container.log</file>
  </logging>
</annotatedContainer>
